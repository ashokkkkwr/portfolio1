#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/tiger/Desktop/Intern/naya/node_modules/.pnpm/typeorm@0.3.20_pg@8.12.0_ts-node@10.9.2_@types+node@20.14.9_typescript@5.5.3_/node_modules/typeorm/node_modules:/mnt/c/Users/tiger/Desktop/Intern/naya/node_modules/.pnpm/typeorm@0.3.20_pg@8.12.0_ts-node@10.9.2_@types+node@20.14.9_typescript@5.5.3_/node_modules:/mnt/c/Users/tiger/Desktop/Intern/naya/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/tiger/Desktop/Intern/naya/node_modules/.pnpm/typeorm@0.3.20_pg@8.12.0_ts-node@10.9.2_@types+node@20.14.9_typescript@5.5.3_/node_modules/typeorm/node_modules:/mnt/c/Users/tiger/Desktop/Intern/naya/node_modules/.pnpm/typeorm@0.3.20_pg@8.12.0_ts-node@10.9.2_@types+node@20.14.9_typescript@5.5.3_/node_modules:/mnt/c/Users/tiger/Desktop/Intern/naya/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typeorm/cli-ts-node-esm.js" "$@"
else
  exec node  "$basedir/../typeorm/cli-ts-node-esm.js" "$@"
fi
